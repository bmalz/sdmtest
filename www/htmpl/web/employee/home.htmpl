					var temp = "";
					var flag;
					<PDM_LIST PREFIX=list FACTORY=pcat WHERE="ss_sym = '$args.CONTRACT'">
						i++;
						if ('$list.in_flag' == 0 && '$list.cr_flag' == 0)
							flag = true;
						temp = '$list.sym';
					</PDM_LIST>
	
					if (i > 0) {
						if (flag)
							return 'Area';
						else
							return 'Exist';
					} else {
						return 'NotExist';
					}
				} else {
					return 'Exist'
				}
			}
			
