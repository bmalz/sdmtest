					out += '<td></td>';
					out += '<td></td>';
				}
				
				out += '</tr>';
				out += '</table>'
				out += '</div>';
			}
		} else {
			out += 'Obiekt nie istnieje';
		}

		jq("#CategoriesArea").html(out);
