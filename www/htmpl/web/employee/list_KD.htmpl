		url += "+KEEP.QBE.IN.ebr_search_text=" + nx_escape(<PDM_FMT ESC_STYLE=C>"$args.QBE.IN.ebr_search_text"</PDM_FMT>);
		url += "+KEEP.QBE.EQ.ebr_primary_order=$args.QBE.EQ.ebr_primary_order";
