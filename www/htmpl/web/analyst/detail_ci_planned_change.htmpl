                "$args.status" == "CANCEL"        ||
                "$args.status" == "USEDISCOVERED" ||
                "$args.status" == "USEDDISCOVERED"||
                "$args.status" == "ACCEPTDISC"    ||
                "$args.status" == "ACCEPTEDDISC"  ||
                "$args.status" == "ACCEPTPLAN"    ||
                "$args.status" == "ACCEPTEDPLAN"  ||
                "$args.status" == "SET"           ||
                "$args.status" == "SETCOMPLETE"   ||
                "$args.status" == "ALLOWED"       ||
                "$args.status" == "NOCHANGE"      ||
                "$args.status" == "ROGUE" >
