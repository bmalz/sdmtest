<PDM_MACRO name=dtlLookupReadonly hdr="TWA" attr="ci_twa_ci" link="yes">
<PDM_MACRO name=dtlStartRow>
<PDM_MACRO name=dtlReadonly hdr="Wiadomo weryfikacji" attr="verification_msg" colspan=4>
