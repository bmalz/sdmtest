function edit_interface_type(value)

{
  if ( value == "1" )
     value = "Employee";

  else if ( value == "2" )
     value = "Customer";

  else if ( value == "3" )
     value = "Analyst";

  else if ( value == "0" )
     value = "";

  rs.data(value);     
}

function edit_interface_typeExp()
{
	return "LIST_LOOKUP: 1," + msgtext("Employee") + ",2," + msgtext("Customer") + ",3," + msgtext("Analyst") + ",0, ";
}
  
