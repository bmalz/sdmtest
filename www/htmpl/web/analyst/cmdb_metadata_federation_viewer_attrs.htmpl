<PDM_MACRO name=cmdbMetadata provider_attr="ufam"                       provider_name_regexp=".*" hide_provider_attr="YES" >
<PDM_MACRO name=cmdbMetadata provider_attr="last_mod_dt"                provider_name_regexp=".*" hide_provider_attr="YES" >
<PDM_MACRO name=cmdbMetadata provider_attr="ambiguity_last_update_date" provider_name_regexp=".*" hide_provider_attr="YES" >
<PDM_MACRO name=cmdbMetadata provider_attr="linked_ci_ambiguity_ci"     provider_name_regexp=".*" hide_provider_attr="YES" >
