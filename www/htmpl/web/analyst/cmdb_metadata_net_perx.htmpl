<PDM_MACRO name=cmdbMetadata category="Attributes" extension="net_perx" attr="number_net_card"        heading="Number of Network Cards" help="The number of expansion cards that have been installed in the available slots in a computer">
<PDM_MACRO name=cmdbMetadata category="Attributes" extension="net_perx" attr="role"                   heading="Role" help="The business function supported by a hardware or network device, for example, production, test">
