<PDM_MACRO name=cmdbMetadata category="Attributes" extension="net_perx" attr="last_mtce_date"         heading="Last Maintenance Date" help="The latest date on which maintenance was performed on a CI">
<PDM_MACRO name=cmdbMetadata category="Attributes" extension="net_perx" attr="mtce_level"             heading="Maintenance Level" help="An indication of the current patch version for this CI">
