	<SCRIPT LANGUAGE="JavaScript">
	  var pageURLFldNum;
	  var queueFldNum;
	  var wasLastURLMandatory = false;
	  var initialPageType;
	
		argPersistentID = "$args.persistent_id";

		function preSaveTrigger()
		{
			return true;
		}
		
