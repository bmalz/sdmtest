			out += "<FRAME SRC='" + ahdtop.cfgCAISD +"/html/empty.html' NAME=tworkframe NORESIZE=NORESIZE MARGINHEIGHT=0 MARGINWIDTH=0 FRAMEBORDER=NO BORDER=0 title=\"Hidden frame - do not navigate here\">";
