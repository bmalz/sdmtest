			out += '	<FRAMESET ROWS="300,*">';
			out += '		<FRAME style="border: 1px #8B939A solid;" SRC="$cgi?OP=DISPLAY_FORM+SID=$SESSION.SID+FID=1+HTMPL=telecentrum_user.htmpl+USERID=' + user_id + '" NAME=user>';
			out += '		<FRAME style="border: 1px #8B939A solid;" SRC="$cgi?OP=DISPLAY_FORM+SID=$SESSION.SID+FID=1+HTMPL=telecentrum_devices.htmpl+USERID=' + user_id + '" NAME=devices>';
			out += '	</FRAMESET>';
			out += '	<FRAMESET ROWS="150,*">';
			out += '		<FRAME style="border: 1px #8B939A solid;" SRC="$cgi?OP=DISPLAY_FORM+SID=$SESSION.SID+FID=1+HTMPL=telecentrum_user_details.htmpl+USERID=' + user_id + '" NAME=user_details>';
			out += '		<FRAME style="border: 1px #8B939A solid;" SRC="$cgi?OP=DISPLAY_FORM+SID=$SESSION.SID+FID=1+HTMPL=telecentrum_categories.htmpl+Owning_Contract=' + Owning_Contract + '" NAME=categories>';
