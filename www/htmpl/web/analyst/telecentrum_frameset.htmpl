<SCRIPT>
	document.writeln(out);
	
	if (typeof(ahdtop.product.frames[0].frames[0]) == "undefined") {
		ahdtop.product.frames[0].location.href = "$cgi?OP=DISPLAY_FORM+SID=$SESSION.SID+FID=1+HTMPL=menubar_tele.htmpl+PHONENUMBER=$args.PHONENUMBER+CanReturn=1";
