function display_YN_value(value)
{
  if ( value == "1" )
     value = "Yes";
  else 
     value = "No";
  rs.data(value);     
}
function display_T_value(value)
{
<PDM_IF "$prop.MultiTenancy" != "off">
    if ( value != "" && tenant != "" )
    {
        value += ahdtop.cfgIndTenant;
    }
</PDM_IF>
    rs.data(value);
}
function display_T_value2(value)
{
<PDM_IF "$prop.MultiTenancy" != "off">
