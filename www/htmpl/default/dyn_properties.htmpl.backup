	function init_properties(factory, form)
	{
		if (typeof sink != "object" &&
		    typeof factory != "undefined" &&
		    typeof form != "undefined")
		{
			var c = form.elements["SET.category"];
			if (typeof c == "undefined" || !c)
			    c = form.elements["KEY.category"];
			    
			sink = new CategorySink(factory, form, c);

			// If c is undefined, the category field is readonly on the form.
			if (typeof c != "undefined") {
				if ((_browser.supportsDOM >= 2) &&
				    (typeof document.implementation == "object") &&
				    (typeof document.implementation.hasFeature == "function") &&
				    (document.implementation.hasFeature("Events", "2.0") == true)) 
				{
					c.addEventListener("change", sink, false);
				}
				else
				{
					// Have to do this the hard way!
					c.onchange = eventProxy;
				}
			}
		}

		var cat_err_msg = '<PDM_FMT ESC_STYLE=C PAD=NO>${AlertMsg:}</PDM_FMT>';
		if ( cat_err_msg == "") {
			sink.update_ss_cat_field('<PDM_FMT ESC_STYLE=C PAD=NO>$args.category.ss_sym</PDM_FMT>', 
				'<PDM_FMT ESC_STYLE=C PAD=NO>$args.category</PDM_FMT>');
		}
		
		sink.clearProperties();
	
