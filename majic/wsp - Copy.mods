////////////////////////////////////////////////////////////////////////
// Factory:   cnt
////////////////////////////////////////////////////////////////////////

OBJECT cnt {
  ATTRIBUTES usp_contact SECONDARY {
    zNRE STRING 10;
    zakceptujacy INTEGER
      {
      ON_NEW DEFAULT 0;
      };
    zcrref BREL zlrel_cr_cnt cnt DYNAMIC {
    	DOMSET MLIST_DYNAMIC;
    	WHERE cnt = ? ;
    	PARAM_NAMES id;
    	};
    zcrref1 BREL zlrel_cr_cnt1 cnt DYNAMIC {
    	DOMSET MLIST_DYNAMIC;
    	WHERE cnt = ? ;
    	PARAM_NAMES id;
    	};
    zcrref2 BREL zlrel_cr_cnt2 cnt DYNAMIC {
    	DOMSET MLIST_DYNAMIC;
    	WHERE cnt = ? ;
    	PARAM_NAMES id;
    	};
    zcrref3 BREL zlrel_cr_cnt3 cnt DYNAMIC {
    	DOMSET MLIST_DYNAMIC;
    	WHERE cnt = ? ;
    	PARAM_NAMES id;
    	};
    zuserAccountControl INTEGER; // do usunięcia!
    zwyl INTEGER
      {
      ON_NEW DEFAULT 0;
      };
  };
};

