////////////////////////////////////////////////////////////////////////
// Factory:   cr
////////////////////////////////////////////////////////////////////////

OBJECT cr {
  ATTRIBUTES Call_Req {
    zacctype1 SREL zacctypes;
    zacctype2 SREL zacctypes;
    zacctype3 SREL zacctypes;
    zacctype4 SREL zacctypes;
    zcntref BREL zlrel_cr_cnt cr DYNAMIC {
    	DOMSET MLIST_DYNAMIC;
    	WHERE cr = ? ;
    	PARAM_NAMES persistent_id;
    };
    zcntref1 BREL zlrel_cr_cnt1 cr DYNAMIC {
    	DOMSET MLIST_DYNAMIC;
    	WHERE cr = ? ;
    	PARAM_NAMES persistent_id;
    };
    zcntref1rules SREL zas;
    zcntref2 BREL zlrel_cr_cnt2 cr DYNAMIC {
    	DOMSET MLIST_DYNAMIC;
    	WHERE cr = ? ;
    	PARAM_NAMES persistent_id;
    };
    zcntref2rules SREL zas;
    zcntref3 BREL zlrel_cr_cnt3 cr DYNAMIC {
    	DOMSET MLIST_DYNAMIC;
    	WHERE cr = ? ;
    	PARAM_NAMES persistent_id;
    };
    zcntref3rules SREL zas;
    zcntref4rules SREL zas;
    zcustomer STRING 256;
    zgroup1 SREL grp;
    zgroup1rules SREL zas;
    zgroup2 SREL grp;
    zgroup2rules SREL zas;
    zgroup3 SREL grp;
    zgroup3rules SREL zas;
    zgroup4 SREL grp;
    zgroup4rules SREL zas;
    zloc SREL loc;
    zlvl STRING 1;
    zmanager1 SREL cnt;
    zmanager2 SREL cnt;
    zmanager3 SREL cnt;
    zmanager4 SREL cnt;
    zmpk SREL cost_cntr
      
      DISPLAY_NAME "MPK";
    zsla SREL zSLA // SRel_Attr_Entry cr.zsla
      
      DISPLAY_NAME "SLA"
      {
      ON_POST_VAL site_set_sla( ) 80;
      };
    zuwagifz STRING 4000 // Uwagi do firmy zewnętrznej
      
      DISPLAY_NAME "uwagiFZ";
  };
};

