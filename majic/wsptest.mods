////////////////////////////////////////////////////////////////////////
// Factory:   ztask (Task)
// Description: Fac_Attr_Entry ztask
////////////////////////////////////////////////////////////////////////

OBJECT ztask {
  DISPLAY_GROUP "macro event stored_query object_notify a";

  DISPLAY_NAME "Task";

  ATTRIBUTES ztask {
    actual_duration DURATION
      
      DISPLAY_NAME "Actual Duration"
      {
      ON_NEW DEFAULT 0;
      };
    asset SREL nr;
    assignee SREL cnt
      SERVICE_PROVIDER_ELIGIBLE 
      DISPLAY_NAME "Assignee";
    assignee_agt SREL agt;
    attached_events QREL atev DYNAMIC {
    	WHERE "obj_id = ? " ;
    	PARAM_NAMES { persisent_id } ;
    	};
    audit_userid SREL cnt;
    chg SREL chg;
    closecode SREL ztaskclosecode
      
      DISPLAY_NAME "Close Code";
    comments STRING 1000
      
      DISPLAY_NAME "Comments";
    completion_date DATE
      
      DISPLAY_NAME "Completion date"
      {
      ON_PRE_VAL check_dates(start_date,completion_date) 80;
      };
    cost INTEGER
      
      DISPLAY_NAME "Cost"
      {
      ON_NEW DEFAULT 0;
      };
    cr SREL cr;
    creator SREL cnt
      
      DISPLAY_NAME "Creator"
      {
      ON_NEW DEFAULT USER;
      };
    date_created DATE
      
      DISPLAY_NAME "Created date"
      {
      ON_NEW DEFAULT NOW;
      };
    del SREL actbool
      
      DISPLAY_NAME "delete_flag"
      {
      ON_NEW DEFAULT 0;
      };
    deliveredServices STRING 1000
      
      DISPLAY_NAME "Delivered Services";
    description STRING 1000
      
      DISPLAY_NAME "Description";
    done_by SREL cnt
      
      DISPLAY_NAME "Done by";
    est_completion_date DATE
      
      DISPLAY_NAME "Estimated completion date"
      {
      ON_PRE_VAL check_date(start_date,est_completion_date) 80;
      };
    est_cost INTEGER
      
      DISPLAY_NAME "Estimated cost"
      {
      ON_NEW DEFAULT 0;
      };
    est_duration DURATION
      
      DISPLAY_NAME "Estimated duration"
      {
      ON_NEW DEFAULT 0;
      };
    expertise INTEGER
      
      DISPLAY_NAME "Expertise"
      {
      ON_NEW DEFAULT 0;
      };
    group_task INTEGER
      
      DISPLAY_NAME "Group task"
      {
      ON_NEW DEFAULT 0;
      };
    installedSoftware STRING 1000
      
      DISPLAY_NAME "Installed Software";
    laborCost INTEGER
      
      DISPLAY_NAME "Labor Cost";
    pamtask INTEGER
      {
      ON_NEW DEFAULT 0;
      };
    parentPERSID STRING 30;
    partsCost STRING 50
      
      DISPLAY_NAME "Parts cost";
    rate INTEGER
      
      DISPLAY_NAME "Rate";
    sdgroup SREL grp
      
      DISPLAY_NAME "Group";
    sequence INTEGER
      REQUIRED;
    spenttime STRING 50
      
      DISPLAY_NAME "Spent Time";
    start_date DATE;
    status SREL tskstat // SRel_Attr_Entry ztask.status
      
      DISPLAY_NAME "Status"
      {
      ON_NEW DEFAULT "WAIT";
      ON_POST_VAL site_att_cr_evt( ) 51 FILTER ( task == 'CRAPPTSK' && pamtask == 1 );
      };
    task SREL tskty
      
      DISPLAY_NAME "Task";
    taskType INTEGER; // nie używać
    taskType2 SREL ztasktype;
    usedParts STRING 1000
      
      DISPLAY_NAME "Used Parts";
    web_url DERIVED ("$(NX_WEB_LINK_URL#%s#ztask)id=" id);
    last_mod_dt DATE { ON_CI SET NOW; };
    last_mod_by SREL cnt { ON_CI SET USER;
                           ON_NEW DEFAULT USER; };
  };

  FACTORY ztask {
    STANDARD_LISTS {
      MLIST OFF;
      RLIST OFF;
    };
    FUNCTION_GROUP "ztask";
    REL_ATTR id;
    COMMON_NAME description;
  };
};

